	<queryString>
		<![CDATA[SELECT 
RECEIPTS.DATENEW,
     TICKETS.TICKETID,
     PAYMENTS.PAYMENT,
     PAYMENTS.TOTAL,
     CUSTOMERS.TAXID,
     CUSTOMERS.NAME
FROM
     RECEIPTS INNER JOIN TICKETS ON RECEIPTS.ID = TICKETS.ID
     INNER JOIN PAYMENTS ON RECEIPTS.ID = PAYMENTS.RECEIPT
     INNER JOIN CUSTOMERS ON TICKETS.CUSTOMER = CUSTOMERS.ID
WHERE
     (PAYMENTS.PAYMENT = 'debt'
  OR PAYMENTS.PAYMENT = 'debtpaid')
ORDER BY
     CUSTOMERS.NAME ASC]]>
	</queryString>
